|   if (! f)
|     return 1;
|   if (((long int) (sizeof (_Float16))) < 0)
|     {
|       long int i = longval ();
|       if (i != ((long int) (sizeof (_Float16))))
| 	return 1;
|       fprintf (f, "%ld", i);
|     }
|   else
|     {
|       unsigned long int i = ulongval ();
|       if (i != ((long int) (sizeof (_Float16))))
| 	return 1;
